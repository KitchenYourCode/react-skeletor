{
  "transform": {
    ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/jest/setupTestFrameworkScriptFile.js",
  "browser": true,
  "verbose": true
}
